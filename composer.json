{
    "name": "morrislaptop/firestore-php",
    "description": "A clone of morrislaptop/firestore-php Firestore SDK for PHP without gRPC",
    "keywords": ["firestore", "google", "sdk", "api", "database"],
    "homepage": "https://github.com/morrislaptop/firestore-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Page Carbajal",
            "email": "ask@pagecarbajal.com"
        }
    ],
    "support": {
        "docs": "https://morrislaptop.github.io/firestore-php/",
        "issues": "https://github.com/morrislaptop/firestore-php/issues",
        "source": "https://github.com/morrislaptop/firestore-php"
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "kreait/firebase-php": "^6.9.0",
        "google/protobuf": "^3.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1",
        "morrislaptop/var-dumper-with-context": "^0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Morrislaptop\\Firestore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morrislaptop\\Firestore\\Tests\\": "tests",
            "Kreait\\Firebase\\Tests\\": "vendor/kreait/firebase-php/tests"
        }
    }
}
